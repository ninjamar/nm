; Expirimenting to differentiate between standard library, python modules and nm modules
;(include py::math)
;(include std::file)
;(include file::test.nm)
(define a 0)
(define o [])
(func handle_char (c) (
    (if (eq (a 250))(
        (define a 0)
    ))
    (if (eq (c "+"))(
        (define a (add (a 1)))
    ))
    (if (eq (c "."))(
        (append o (chr (a)))
    ))
))
(foreach program item (
    (handle_char (item))
))
